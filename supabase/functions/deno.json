{
  "tasks": {
    "check-functions": "deno check */*.ts --reload",
    "check-format-functions": "deno fmt --check */*.ts",
    "lint-functions": "deno lint */*.ts",
    "test-functions": "deno test */*.test.ts"
  },
  "imports": {},
  "lint": {
    "rules": {
      "exclude": ["no-import-prefix", "no-unversioned-import"]
    }
  },
  "unstable": ["net", "http"],
  "nodeModulesDir": "auto"
}
